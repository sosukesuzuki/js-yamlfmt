{
  "name": "@js-yamlfmt/node",
  "packemon": {
    "platform": "node"
  },
  "types": "./dts/index.d.ts",
  "main": "./mjs/index.mjs",
  "type": "module",
  "bin": {
    "node-yamlfmt": "./mjs/index.mjs"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": {
        "import": "./mjs/*.mjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./mjs/index.mjs"
      }
    }
  },
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@js-yamlfmt/wasm": "workspace:^",
    "globby": "^13.1.2",
    "meow": "^10.1.3",
    "tinypool": "^0.2.4"
  }
}
